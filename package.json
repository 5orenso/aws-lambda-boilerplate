{
    "name": "aws-lambda-boilerplate",
    "description": "A good start to make a lambda function",
    "version": "1.0.0",
    "homepage": "https://github.com/5orenso/aws-lambda-boilerplate",
    "repository": {
        "type": "git",
        "url": "https://github.com/5orenso/aws-lambda-boilerplate"
    },
    "bugs": {
        "url": "https://github.com/5orenso/aws-lambda-boilerplate/issues"
    },
    "main": "index.js",
    "engines": {
        "node": ">=8.11.0"
    },
    "jest": {
        "collectCoverageFrom": [
            "*.js"
        ],
        "coveragePathIgnorePatterns": [
            "/node_modules/"
        ],
        "coverageThreshold": {
            "global": {
                "statements": 100,
                "branches": 100,
                "functions": 100,
                "lines": 100
            }
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint && npm test"
        }
    },
    "scripts": {
        "lint": "eslint ./index.js",
        "test": "jest --coverage",
        "test:watch": "jest --watch",
        "build": "bash ./bin/build-release.sh"
    },
    "devDependencies": {
        "aws-sdk-mock": "^4.3.0",
        "eslint": "^5.12.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-config-standard-preact": "^1.1.6",
        "eslint-plugin-import": "^2.2.0",
        "eslint-plugin-jsx-a11y": "^4.0.0",
        "eslint-plugin-react": "^6.10.3",
        "husky": "^1.3.1",
        "jest": "^23.6.0",
        "time-grunt": "^2.0.0"
    },
    "keywords": [],
    "dependencies": {
        "aws-sdk": "^2.3.8"
    }
}
